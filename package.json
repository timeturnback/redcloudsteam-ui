{
  "name": "@redcloudsteam/ui",
  "version": "1.0.6",
  "description": "Global loading screen for React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepack": "tsup",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timeturnback/@redcloudsteam/ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "react",
    "global",
    "loading"
  ],
  "author": "Redclouds",
  "license": "MIT",
  "devDependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@babel/core": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.12",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "parcel": "^2.6.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "release-it": "^15.7.0",
    "tsup": "^6.5.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "bugs": {
    "url": "https://github.com/timeturnback/@redcloudsteam/ui/issues"
  },
  "homepage": "https://github.com/timeturnback/@redcloudsteam/ui#readme",
  "dependencies": {},
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    }
  }
}
